{
  "scripts": {
    "start": "node index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest",
    "server": "cross-env DEBUG=app nodemon --exec babel-node src/app.js",
    "server:prod": "cross-env NODE_ENV=production nodemon --exec babel-node src/app.js",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "docs:post": "opn docs/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-stage-1": "^7.8.3",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.3",
    "apidoc": "^0.29.0",
    "apidoc-markdown": "^6.0.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "jest-cli": "^27.0.6",
    "mockgoose": "^8.0.4",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "supertest-as-promised": "^4.0.2"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "joi": "^17.4.2",
    "mongoose": "^5.13.7"
  }
}
