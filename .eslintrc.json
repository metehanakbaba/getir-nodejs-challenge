{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "jest": true,
    "jasmine": true
  },
  "rules": {
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-var": [
      "off"
    ],
    "one-var": [
      "off"
    ],
    "import/prefer-default-export": "off",
    "import/no-cycle": "off"
  }
}
