{
  "extends": "airbnb-base",
  "env": {
    "es6": true
  },
  "rules": {
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-var": [
      "off"
    ],
    "one-var": [
      "off"
    ]
  },
  "env": {
  "jest": true,
  "jasmine": true
}
}
